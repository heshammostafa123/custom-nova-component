{
    "name": "seoegypt/nova-custom-components",
    "type": "laravel-library",
    "description": "A Laravel Nova resource tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "oomphinc/composer-installers-extender": "*"
    },
    "require-dev": {
        "oomphinc/composer-installers-extender": "*"
    },
    "autoload": {
        "psr-4": {
            "Seoegypt\\NovaCustomComponents\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seoegypt\\NovaCustomComponents\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
